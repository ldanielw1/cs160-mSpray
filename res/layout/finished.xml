                android:id="@+id/finished_rooms_sprayed2_label"
                android:layout_width="0dp"
                android:layout_weight="0.7"
                android:text="  Rooms Sprayed"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/finished_rooms_sprayed2_value"
                android:layout_width="0dp"
                android:layout_weight="0.3"
                android:text=" ? "
                android:textSize="16sp" />
        </TableRow>

        <TableRow
            android:id="@+id/finished_shelters_sprayed2_row"
