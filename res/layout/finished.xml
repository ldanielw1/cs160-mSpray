<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:screenOrientation="portrait" >

    <TextView
        android:id="@+id/finished_textview_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="12dp"
        android:gravity="center"
        android:text="@string/dataRecorded"
        android:textSize="28sp" />

    <TableLayout
        android:id="@+id/finished_table"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/finished_textview_title"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp" >
        
        <TableRow
            android:id="@+id/finished_foreman_row"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp" >

            <TextView
                android:id="@+id/finished_foreman_label"
                android:layout_width="0dp"
                android:layout_weight="0.7"
                android:text="Foreman"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/finished_foreman_value"
                android:layout_width="0dp"
                android:layout_weight="0.3"
                android:text=" ? "
                android:textSize="24sp" />
        </TableRow>

        <TableRow
            android:id="@+id/finished_sprayer_row"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp" >

            <TextView
                android:id="@+id/finished_sprayer_label"
                android:layout_width="0dp"
                android:layout_weight="0.7"
                android:text="Sprayer"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/finished_sprayer_value"
                android:layout_width="0dp"
                android:layout_weight="0.3"
                android:text=" ? "
                android:textSize="24sp" />
        </TableRow>
        
         <TableRow
            android:id="@+id/finished_rooms_sprayed_row"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp" >

            <TextView
                android:id="@+id/finished_rooms_sprayed_label"
                android:layout_width="0dp"
                android:layout_weight="0.7"
                android:text="  Rooms Sprayed"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/finished_rooms_sprayed_value"
                android:layout_width="0dp"
                android:layout_weight="0.3"
                android:text=" ? "
                android:textSize="16sp" />
        </TableRow>

        <TableRow
            android:id="@+id/finished_shelters_sprayed_row"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp" >

            <TextView
                android:id="@+id/finished_shelters_sprayed_label"
                android:layout_width="0dp"
                android:layout_weight="0.7"
                android:text="  Shelters Sprayed"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/finished_shelters_sprayed_value"
                android:layout_width="0dp"
                android:layout_weight="0.3"
                android:text=" ? "
                android:textSize="16sp" />
        </TableRow>
        
        <TableRow
