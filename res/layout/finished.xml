        </TableRow>
        
        <TableRow
            android:id="@+id/finished_can_refilled2_row"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp" >

            <TextView
                android:id="@+id/finished_can_refilled2_label"
                android:layout_width="0dp"
                android:layout_weight="0.7"
                android:text="  Refill can"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/finished_can_refilled2_value"
                android:layout_width="0dp"
                android:layout_weight="0.3"
                android:text=" ? "
                android:textSize="16sp" />
        </TableRow>

        <TableRow
            android:id="@+id/finished_latitude_row"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp" >

            <TextView
                android:id="@+id/finished_latitude_label"
                android:layout_width="0dp"
                android:layout_weight="0.7"
