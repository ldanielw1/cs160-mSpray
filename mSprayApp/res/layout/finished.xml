                android:layout_height="wrap_content"
                android:padding="4dp" >

                <TextView
                    android:id="@+id/finished_sprayer_label"
                    android:layout_width="0dp"
                    android:layout_weight="0.7"
                    android:text="@string/sprayer"
                    android:textColor="#fff"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/finished_sprayer_value"
                    android:layout_width="0dp"
                    android:layout_weight="0.3"
                    android:text=""
                    android:textColor="#fff"
                    android:textSize="20sp" />
            </TableRow>

            <TableRow
