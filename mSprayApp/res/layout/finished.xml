                    android:textColor="#fff"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/finished_unsprayed_reason_value"
                    android:layout_width="0dp"
                    android:layout_weight="0.3"
                    android:text=""
                    android:textColor="#fff"
                    android:textSize="16sp" />
            </TableRow>

            <TableRow
                android:id="@+id/finished_latitude_row"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="4dp" >

                <TextView
                    android:id="@+id/finished_latitude_label"
                    android:layout_width="0dp"
