<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/darkgray"
    android:screenOrientation="portrait" >

    <TextView
        android:id="@+id/finished_textview_title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:background="@drawable/grad3"
        android:gravity="center"
        android:text="@string/dataRecorded"
        android:textSize="28sp" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@+id/finished_layout_innerLinearLayout"
        android:layout_below="@+id/finished_textview_title"
        android:scrollbars="none" >

        <TableLayout
            android:id="@+id/finished_table"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="8dp" >

            <TableRow
                android:id="@+id/finished_chemical_row"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="4dp" >

                <TextView
                    android:id="@+id/finished_chemical_label"
                    android:layout_width="0dp"
                    android:layout_weight="0.7"
                    android:text="@string/chemical"
                    android:textColor="#fff"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/finished_chemical_value"
                    android:layout_width="0dp"
                    android:layout_weight="0.3"
                    android:text=""
                    android:textColor="#fff"
                    android:textSize="20sp" />
            </TableRow>

            <TableRow
                android:id="@+id/finished_foreman_row"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="4dp" >

                <TextView
                    android:id="@+id/finished_foreman_label"
                    android:layout_width="0dp"
                    android:layout_weight="0.7"
                    android:text="@string/foreman"
                    android:textColor="#fff"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/finished_foreman_value"
                    android:layout_width="0dp"
