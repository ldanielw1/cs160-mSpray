                android:padding="4dp" >

                <TextView
                    android:id="@+id/finished_rooms_sprayed_label"
                    android:layout_width="0dp"
                    android:layout_weight="0.7"
                    android:text="@string/roomsSprayedLabel2"
                    android:textColor="#fff"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/finished_rooms_sprayed_value"
